    "version": "0.2.0",
    "configurations": [
        {
            "name": "ApiLogicServer",
            "type": "python",
            "env": {"APILOGICPROJECT_DEBUG": "False"},
            "request": "launch",
            "program": "api_logic_server_run.py",
            "redirectOutput": true,
            "justMyCode": false,
            "args": ["--flask_host=localhost", "--port=5656", "--swagger_host=localhost", "--verbose=False"],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "ApiLogicServer ENV",
            "type": "python",
            "env": {
                "APILOGICPROJECT_DEBUG": "False",
                "VERBOSE": "true",
                "SQLALCHEMY_DATABASE_URI": "mssql+pyodbc://valhuber:PWD@mysqlserver-nwlogic.database.windows.net:1433/nwlogic?driver=ODBC+Driver+17+for+SQL+Server&trusted_connection=no"
            },
            "request": "launch",
            "program": "api_logic_server_run.py",
            "redirectOutput": true,
            "justMyCode": false,
            "args": ["--flask_host=localhost", "--port=5656", "--swagger_host=localhost", "--verbose=False"],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "ApiLogicServer - No Security (e.g., for behave tests)",
            "type": "python",
            "request": "launch",
            "program": "api_logic_server_run.py",
            "redirectOutput": true,
            "env": {"SECURITY_ENABLED": "False"},
            "justMyCode": false,
            "args": ["--flask_host=localhost", "--port=5656", "--swagger_host=localhost", "--verbose=False"],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Behave Run",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/test/api_logic_server_behave",
            "program": "${workspaceFolder}/test/api_logic_server_behave/behave_run.py",
            "redirectOutput": true,
            "args": [ "--outfile=${workspaceFolder}/test/api_logic_server_behave/logs/behave.log"],
            "justMyCode": false,            
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Windows Behave Run",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}\\test\\api_logic_server_behave\\features",
            "program": "${workspaceFolder}\\test\\api_logic_server_behave\\behave_run.py",
            "redirectOutput": true,
            "args": [ "--outfile=${workspaceFolder}\\test\\api_logic_server_behave\\logs\\behave.log"],
            "justMyCode": false,            
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Perform Customizations",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "perform_customizations.py",
            "redirectOutput": true,
            "args": [ "go"],
            "justMyCode": false,            
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Behave Scenario",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/test/api_logic_server_behave",
            "program": "${workspaceFolder}/test/api_logic_server_behave/behave_run.py",
            "redirectOutput": true,
            "args": [ "--outfile=${workspaceFolder}/test/api_logic_server_behave/logs/behave.log",
                "--name=Clone Existing Order"],
            "justMyCode": false,            
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Behave Logic Report",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/test/api_logic_server_behave",
            "program": "${workspaceFolder}/test/api_logic_server_behave/behave_logic_report.py",
            "redirectOutput": true,
            "justMyCode": false,            
            "args": ["run"
                ,"--prepend_wiki=reports/Behave Logic Report Intro.md"
                ,"--wiki=reports/Behave Logic Report.md"
            ],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Codespaces-ApiLogicServer",
            "type": "python",
            "request": "launch",
            "program": "api_logic_server_run.py",
            "redirectOutput": true,
            "justMyCode": false,
            "args": [
                "--flask_host=localhost", "--port=5656", "--verbose=True",
                "--swagger_host=valhuber-tutorial-apilogicproject-jjr5qwg72vxg-5656.githubpreview.dev", 
                "--swagger_port=443", "--http_type=https"
            ],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Basic Server Test",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test/basic/server_test.py",
            "redirectOutput": true,
            "justMyCode": false,
            "args": ["go"],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "redirectOutput": true,
            "justMyCode": false,            
            "module": "enter-your-module-name"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "redirectOutput": true,
            "justMyCode": false,            
            "console": "internalConsole"
        }
    ]
}
